{
  "presets": [
    [
      "@babel/preset-env",
      {
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "modules": false,
        "useBuiltIns": "entry"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}
